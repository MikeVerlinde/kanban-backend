generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
}

model User {
  id          Int       @id @default(autoincrement())
  username    String    @unique
  password    String
}

model Lane {
  id          Int       @id @default(autoincrement())
  name        String
  position    Int       @unique
  createdAt   DateTime
}